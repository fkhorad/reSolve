# Basic
process: 2
DYprocess: 3 !1=W+, 2=W-, 3=W+ or W-, 4 = Z only, 5 = Z or gamma*
DYnarrowwidthapprox: 0
resum_flag: 0
order: 0
pdf_flag: 80
CM_energy: 1960.
verbosity: 1 !Amount of output text
ih1: -1 ! hadron type for beam1 (1=proton  -1=antiproton)
ih2: 1 ! hadron type for beam2 (1=proton  -1=antiproton)
save_events: 2
workdir: Wpm_Born_Tevatron/
# Scales
mu_R: 80.398 ! renormisation scale (GeV)
mu_S: 1. ! resummation scale (GeV)
muR_flag: 0
mu_F: 80.398 ! factorisation scale (GeV)
muF_flag: 0
# Integration
maxeval: 100000 ! Approximate max number of integral iterations
nstart: 10000 ! Number of integral iterations in first evaluation
nincrease: 10000 ! Number of extra integral iterations in each further evaluation
integrator_flag: 2 !1=default, 2=cuba
multi_machine: 0
seed: 0
# Resummation
boxflag: 0
gqnp: 0.
ggnp: 0.
QQ_Min: 2.
QQ_Max: 200.
QT_Min: 0.
QT_Max: 200.
eta_Min: -3.
eta_Max: 3.
# DrellYan
crack1: 1.37 ! etacut crack1 general cut
crack2: 1.37 ! etacut crack2 general cut 
pTecut: 20.0 ! general DY W cut
pTmisscut: 25.0 ! general DY W cut
etaecut: 2 ! general W cut
tmasscut: 0.0 ! general W cut
# Histograms
histo: eta 50 -2. 2.
